

// selection sorting 
# include <stdio.h>
void main ()
{
int i,j,t,r,n,s,num,temp,min,pos;
int a[100];
printf("enter the array size\n");
scanf("%d",&n);
printf("enter the array\n");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
for(i=0;i<n-1;i++)
{
pos=i;
for (j=i+1;j<n;j++)
{
if (a[pos]>a[j])
{
pos=j;
}
}
if(pos!=i)
{
temp=a[i];
a[i]=a[pos];
a[pos]=temp;
}
}
for(i=0;i<n;i++)
{
printf("%d\n",a[i]);
}
}
